  "description": "Lifespeed platform",
  "main": "server.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "blacklist": [
            "regenerator"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "browser-sync": "^2.7.10",
    "browserify": "^10.2.4",
    "debug": "^2.2.0",
    "flux": "^2.0.3",
    "jquery": "^2.1.4",
    "koa-better-body": "^1.0.17",
    "koa-compose": "^2.3.0",
    "koa-compress": "^1.0.8",
    "koa-cors": "0.0.16",
    "koa-etag": "^2.0.0",
    "koa-helmet": "^0.1.0",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.2.2",
    "koa-passport": "^1.1.6",
    "koa-router": "^5.1.2",
    "koa-send": "^1.3.1",
    "lodash": "^3.9.3",
    "moment": "^2.10.3",
    "node-uuid": "^1.4.3",
    "object-assign": "^3.0.0",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.3",
    "react-grid-layout": "^0.8.3",
    "react-zeroclipboard": "^1.1.0",
    "reactify": "^1.1.1",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "sweetalert": "^1.0.1",
    "vanilla-masker": "^1.0.9"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.15",
    "babelify": "^6.1.2",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "keymirror": "^0.1.1",
    "koa": "^0.21.0",
    "koa-conditional-get": "^1.0.3"
  },
  "scripts": {
    "postinstall": "npm run version && npm run bower && npm run gulpBuild",
    "version": "node -v",
    "bower": "bower install --config.interactive=false",
    "gulpBuild": "cd gulp && npm install --verbose",
    "build": "cd gulp && NODE_ENV=development && gulp",
    "start": "node server",
    "production": "cd gulp && NODE_ENV=production && gulp production"
  },
  "repository": {
    "type": "git",
