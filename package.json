{
  "name": "Platform",
  "version": "0.0.1",
  "description": "Lifespeed platform",
  "main": "gruntfile.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "blacklist": [
            "regenerator"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "browser-sync": "^2.7.10",
    "browserify": "^10.2.4",
    "consolidate": "^0.13.1",
    "debug": "^2.2.0",
    "envify": "^3.4.0",
    "express": "^4.12.4",
    "flux": "^2.0.3",
    "jquery": "^2.1.4",
    "lodash": "^3.9.3",
    "moment": "^2.10.3",
    "node-uuid": "^1.4.3",
    "normalize": "^0.3.1",
    "object-assign": "^3.0.0",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.3",
    "react-grid-layout": "^0.8.3",
    "react-zeroclipboard": "^1.1.0",
    "reactify": "^1.1.1",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "sweetalert": "^1.0.1",
    "vanilla-masker": "^1.0.9"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.15",
    "babelify": "^6.1.2",
    "compression": "^1.5.0",
    "envify": "^3.4.0",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "keymirror": "^0.1.1"
  },
  "scripts": {
    "postinstall": "npm run version && npm run bower && npm run gulpBuild",
    "version": "node -v",
    "bower": "bower install --config.interactive=false",
    "gulpBuild": "cd gulp && npm install --verbose",
    "build": "cd gulp && NODE_ENV=development && gulp",
    "start": "node server",
    "production": "cd gulp && NODE_ENV=production && gulp production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifespeed/lifespeed.web"
