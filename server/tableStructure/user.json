user table
1. user_uuid (primary key)
2. first name
3. last name
4. background-check url link
5. any government photo document url link
6. gender
7. min money per shift

accounts table
1. user_uuid
2. bcrypt hash
3. user email

rn license table
1. user_uuid
2. license state
3. license number
4. license status

work history table
1. user_uuid
2. facility_uuid
3. work length

phone number table
1. user_uuid || address_uuid
2. phone title
3. phone number

address table
1. address_uuid (primary key)
2. address
3. city
4. zip
5. state

profile pic table
1. user_uuid
2. profile pic thumbnail url link
3. profile pic bigger url link

location geohash table
1. user_uuid || address_uuid
2. geohash int

schedule table
1. user_uuid
2. shift (7 or 19)
3. availiable days
4. shift duration

rating table
1. facility_uuid || user_uuid
2. rating
3. date
4. author_uuid (facility_uuid || user_uuid)

comment table
1. comment_uuid (primary key)
2. user_uuid || facility_uuid
3. author_uuid
4. comment date
5. comment

comment tree
1. parent_comment_uuid
2. child_comment_uuid

facility table
1. facility_uuid (primary key)
2. address_uuid
3. facility name

facility units table
1. facility_uuid
2. specialty_uuid

facility units list table
1. specialty_uuid (primary key)
2. facility specialties
